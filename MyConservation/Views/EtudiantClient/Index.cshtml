@model IEnumerable<MyConservation.Models.Etudiant>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/LayoutsEtudiant/_Default.cshtml";
}
<header id="head" class="secondary"></header>


     @if (TempData["AlertMessage"] != null) { 
    
        <div class="alert alert-success">
     <strong>Success !</strong>   @TempData["AlertMessage"]

    </div>
    
    }
     @if (TempData["AlertMessage2"] != null) { 
    
        <div class="alert alert-success">
     <strong>Erreur !</strong>   @TempData["AlertMessage2"]

    </div>
    
    }
 <div class="Container Acceuil">
   <div class="row">
			<div class="col-lg-12">
				<h1 class="page-header" id="Acc"><i class="fa fa-first-order" aria-hidden="true"></i>Votre information personnelle</h1>
			</div>
		</div><!--/.row-->
 <br /><br />
<table id="exemple" class="table table-striped table-bordered ">
    <thead id="Acc">
    <tr>
        <th>#</th>
        <th>
            Nom
        </th>
        <th>
            Prenom
        </th>
        <th>
           Adresse
        </th>
        <th>
           Email
        </th>
        
        <th>
            Sexe
        </th>
        <th>
            Nationalite
        </th>
        
        <th>Actions</th>
    </tr>
        </thead>
    <tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.prenom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.adresse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.sexe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nationalite)
        </td>
        
        <td>

                     
            @Html.ActionLink("Modifier", "Edit", new { id = item.id }, new { @class="btn btn-info"}) |
            @Html.ActionLink("Details", "Details", new { id=item.id }, new { @class="btn btn-warning"}) |
            @Html.ActionLink("Supprimer", "Delete", new { id=item.id }, new { @class="btn btn-danger"})
                
           
        </td>
    </tr>
}
</tbody>
</table>
    </div>
@section scripts{
    <script src="~/Scripts/DataTable.js"></script>
    
    }