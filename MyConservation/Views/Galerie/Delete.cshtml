@model MyConservation.Models.Galery

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Layouts/_Default.cshtml";
}

<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
<h2 id="Acc"> Les Details d'un image</h2>

<fieldset>
    <legend>Galery</legend>
    <div class="display-field">
     <h4>Identifiant:   @Html.DisplayFor(model => model.id)</h4>
    </div>
    <div class="display-field">
     <h4>Photo:   @Html.DisplayFor(model => model.photo)</h4>
    </div>
    <div class="display-label">
         @Html.DisplayNameFor(model => model.Evenement1.titreEvenement)
    </div>
    <div class="display-field">
     <h4>Titre d'evenement   @Html.DisplayFor(model => model.Evenement1.titreEvenement)</h4>
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <p>
        <button type="submit" value="Delete" class="btn btn-danger">Supprimer</button>
         |
        @Html.ActionLink("Retourne sur la liste", "Index")
    </p>
}
</div>
