@model MyConservation.Models.Etudiant

@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Layouts/_Default.cshtml";
}
<div class="col-sm-9 col-sm-offset-3 col-lg-10 col-lg-offset-2 main">
<h1 id="Acc">Supprime un etudiant</h1>

<h3>Vous avez besoin de supprimer le compte d'etudiant ?</h3>
<fieldset>
    <legend>Etudiant</legend>

    <div class="display-label">
        <h3> Nom : @Html.DisplayFor(model => model.nom) </h3>
    </div>
    
    <div class="display-label">
       <h3> Prenom :  @Html.DisplayFor(model => model.prenom)</h3>
    </div>

    <div class="display-field">
     <h3>Adresse :  @Html.DisplayFor(model => model.adresse)</h3> 
    </div>

    <div class="display-field">
      <h3>Email :  @Html.DisplayFor(model => model.email)</h3> 
    </div>

   <div class="display-field">
      <h3>Sexe :  @Html.DisplayFor(model => model.sexe) </h3>
    </div>

    <div class="display-field">
      <h3>Nationalite :  @Html.DisplayFor(model => model.nationalite) </h3>
    </div>

    <div class="display-field">
    <h3>Mot de Passe :    @Html.DisplayFor(model => model.password)
    </div>
</fieldset>
@using (Html.BeginForm()) {
    <button type="submit" value="Delete" class="btn btn-danger">Supprimer</button>
              
        @Html.ActionLink("Retour sur la liste", "Index")
}
</div>