@{
    ViewBag.Title = "Downloads";
    Layout = "~/Views/LayoutsEtudiant/_Default.cshtml";
}

<header id="head" class="secondary"></header>  
 
<div class="container Acceuil" >
		
                <div class="clearfix">
    
  @model List<string>  
<h2 id="Acc">Consulter et Télècharger les documents publiées</h2>  
<table id="exemple" class="table table-striped table-bordered col-md-4  ">
    <thead>
    <tr>  
        <th>Titre du document</th>  
          
    </tr> 
        </thead>
    <tbody> 
    @for (var i = 0; i <= Model.Count - 1; i++)  
    {   
        <tr>  
            <td> <h5>@Model[i].ToString() </h5></td>  
            <td>@Html.ActionLink("Télècharge directement", "Download", new { ImageName = @Model[i].ToString() }) </td>  
            
        </tr>  
    }  
        </tbody>
</table>
</div>
   </div>
                  
 
	<!-- /container --> 

@section scripts{
    <script src="~/Scripts/DataTable.js"></script>
    
    }